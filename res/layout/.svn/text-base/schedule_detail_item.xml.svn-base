<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:paddingRight="20dp"
    android:gravity="right" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/sche_detail_item_imageSrc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <TextView
            android:id="@+id/sche_detail_item_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <TextView
            android:id="@+id/sche_detail_item_txtAddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <TextView
            android:id="@+id/sche_detail_item_txtContent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <TextView
            android:id="@+id/sche_detail_item_txtPhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <TextView
            android:id="@+id/sche_detail_item_txtLink"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <TextView
            android:id="@+id/sche_detail_item_txtMapxd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <TextView
            android:id="@+id/sche_detail_item_txtMapyd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/sche_detail_item_txtYear"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="2013"
                android:textColor="#000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/G3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text=". "
                android:textColor="#000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/sche_detail_item_txtMonth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="10"
                android:textColor="#000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/G2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text=". "
                android:textColor="#000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/sche_detail_item_txtDay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="21"
                android:textColor="#000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/sche_detail_item_txtWeek"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="5dp"
                android:text="월"
                android:textColor="#000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/G1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:text="요일"
                android:textColor="#000"
                android:textSize="17sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/sche_detail_item_txtHourd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:text="4"
                android:textColor="#000"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/G5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:text="시 "
                android:textColor="#000"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/sche_detail_item_txtMinute"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:text="30"
                android:textColor="#000"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/G4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:layout_marginTop="2dp"
                android:text="분"
                android:textColor="#000"
                android:textSize="13sp" />
        </LinearLayout>

        <FrameLayout
            android:layout_width="240dp"
            android:layout_height="180dp"
            android:layout_gravity="right"
            android:layout_marginTop="5dp" >

            <ImageView
                android:id="@+id/sche_detail_item_image"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:src="@drawable/gallery512"/>

            <FrameLayout
                android:layout_width="240dp"
                android:layout_height="60dp"
                android:layout_gravity="right"
                android:background="#88000000" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/sche_detail_item_txtTitle"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:ellipsize="end"
                        android:gravity="right"
                        android:singleLine="true"
                        android:text="제목"
                        android:textColor="#fff"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/sche_detail_item_txtPlaceName"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="2dp"
                        android:ellipsize="end"
                        android:gravity="right"
                        android:singleLine="true"
                        android:text="장소명"
                        android:textColor="#9E9E9E"
                        android:textSize="15sp" />
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
                <ImageView
                    android:layout_marginLeft="5dp"
                android:id="@+id/sche_detail_item_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="centerInside"
                android:src="@drawable/shedule_line"/>
  

</LinearLayout>