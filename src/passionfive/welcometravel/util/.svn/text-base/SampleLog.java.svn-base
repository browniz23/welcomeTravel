package passionfive.welcometravel.util;

import android.util.Log;

public class SampleLog {
	static final String TAG = "SampleProject";

	public static void LOG_D(String msg) {
		Log.d(TAG, "SampleProject Debug : "+msg);
	}
	
	public static void LOG_E(String msg) {
		Log.e(TAG, "SampleProject Error : "+msg);
	}
	
	public static void LOG_I(String msg) {
		Log.i(TAG, "SampleProject Info : "+msg);
	}
	
	public static void LOG_V(String msg) {
		Log.v(TAG, "SampleProject Verbose : "+msg);
	}
	
	public static void LOG_W(String msg) {
		Log.w(TAG, "SampleProject Warning : "+msg);
	}
}
